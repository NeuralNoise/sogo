#!/bin/sh

set -e

case "$1" in
    configure)
        service zentyal webserver restart || true
        service zentyal sogo restart || true
        ;;

    abort-upgrade|abort-remove|abort-deconfigure)
        ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 0
    ;;
esac

#DEBHELPER#

exit 0
